<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC

        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
        <hibernate-mapping>
        <class name="com.entity.Score" table="t_score">
        <id name="id" type="integer" column="id">
        <generator class="identity"/>
        </id>
        <property name="year" type="integer" column="year"/>
        <property name="course_id" type="int" column="course_id"/>
        <property name="score" type="double" column="score"/>
        <property name="stu_no" type="long" column="stu_no"/>
        <property name="term" type="string" column="term"/>
        </class>
        </hibernate-mapping>